package String;

public class removedublicate {

	public static void main(String[] args) {
		
		String str = "hello world Prince Kumar";
        int sl = str.length();
        
        String res = "";
        
        for(int i = 0; i < sl; i++) {
            // If the character is not already in the result string, add it.
            if(res.indexOf(str.charAt(i)) == -1 ) {
            	if (str.charAt(i) != ' ')) {
            		continue;
            	}else {
            		
            		res += str.charAt(i);
            	}
            }
        }
        
        System.out.println(res);

	}

}
