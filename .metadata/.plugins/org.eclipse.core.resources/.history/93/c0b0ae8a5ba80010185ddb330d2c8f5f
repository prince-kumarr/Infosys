package String;


public class removedub {

    public static void main(String[] args) {
        
        String str = "Ahello world oleFa";
        int sl = str.length();
        
        String res = "";
        
        for(int i = 0; i < sl; i++) {
            char ch = str.charAt(i);
            
            // Keep all spaces, and remove duplicates for other characters
            if(ch == ' ' || res.indexOf(ch) == -1) {
                res += ch;
            }
        }
        
        System.out.println(res);
    }
}
